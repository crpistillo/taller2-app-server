server {
    listen 80;
    server_name <tu nombre de dominio>;
    location /static {
        alias /root/tutorial-flask/app/static;
    }
    location /media {
        alias /root/tutorial-flask/media;
    }
    location / {
        include proxy_params;
        proxy_pass http://unix:/root/tutorial-flask/miniblog.sock;
    }
}